#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
        char str[100];
    int i = 0,count=0;
    fgets(str,sizeof(str),stdin);
    while(str[i] != '\0'){
        char ch = str[i];
        if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u' || ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U'){
           
               count++;
               }
           i++;
           }
           printf("%d",count);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
