#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>



int calculateSquare(int n) {
    return n * n;
}

int main() {
    int n;
    scanf("%d", &n);
    if (n >= 1 && n <= 9) {
        int square = calculateSquare(n);
        printf("Square of %d is %d\n", n, square);
    } else {
        printf("Invalid Input\n");
    }
    return 0;

}
    
